s=input().lower().replace(" ","")
r=input().lower().replace(" ","")
res={}
res1={}
for char in s:
    res[char]=res.get(char,0)+1
for char in r:
    res1[char]=res1.get(char,0)+1
def check_anagram(res,res1):
    if len(res)!=len(res1):
        return False
    for char in res:
        if res[char]!=res1[char]:
            return False
    return True
print(check_anagram(res,res1))
